{
  "name": "session-token-website",
  "description": "Session Token Website",
  "license": "GPL-3.0-only",
  "scripts": {
    "build": "yarn build:production",
    "build-next": "NEXT_PUBLIC_SITE_ENV=production NODE_ENV=production next build",
    "build-next:safe": "yarn format-full && yarn type-check && yarn build-next",
    "build:dev": "NEXT_PUBLIC_ENV_FLAG=dev yarn build-next:safe",
    "build:production": "NEXT_PUBLIC_ENV_FLAG=prd yarn build-next:safe",
    "build:qa": "NEXT_PUBLIC_ENV_FLAG=qa yarn build-next:safe",
    "build:staging": "NEXT_PUBLIC_ENV_FLAG=stg yarn build-next:safe",
    "clean": "rm -rf .next",
    "dev": "NEXT_PUBLIC_ENV_FLAG=dev next",
    "format": "yarn prettier --list-different --write \"**/*.{css,html,js,json,md,ts,tsx}\"",
    "format-full": "yarn format && yarn lint",
    "lint": "eslint .",
    "prepare": "husky install",
    "start": "yarn start:production",
    "start-next": "NEXT_PUBLIC_SITE_ENV=production NODE_ENV=production next start --hostname 127.0.0.1",
    "start:dev": "NEXT_PUBLIC_ENV_FLAG=dev yarn start-next",
    "start:production": "NEXT_PUBLIC_ENV_FLAG=prd yarn start-next",
    "start:qa": "NEXT_PUBLIC_ENV_FLAG=qa yarn start-next",
    "start:staging": "NEXT_PUBLIC_ENV_FLAG=stg yarn start-next",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@portabletext/react": "3.0.12",
    "@sanity/client": "6.15.5",
    "@sanity/icons": "2.11.2",
    "@sanity/image-url": "1.0.2",
    "@sanity/orderable-document-list": "^1.2.1",
    "@sanity/preview-kit": "5.0.33",
    "@sanity/vision": "3.34.0",
    "@splinetool/react-spline": "4.0.0",
    "@splinetool/runtime": "1.7.4",
    "@vercel/og": "^0.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "2.1.0",
    "cssnano": "6.1.0",
    "framer-motion": "11.0.14",
    "lodash": "^4.17.21",
    "mini-xml": "^1.2.0",
    "moment": "^2.30.1",
    "next": "^14.2.4",
    "next-sanity": "8.4.2",
    "path-to-regexp": "^6.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-intersection-observer": "9.8.1",
    "react-smooth-collapse": "^2.1.2",
    "react-tracked": "1.7.14",
    "react-use": "^17.5.0",
    "sanity": "3.34.0",
    "sanity-plugin-asset-source-unsplash": "2.0.1",
    "sanity-plugin-iframe-pane": "2.6.0",
    "sass": "1.72.0",
    "sharp": "0.32.6",
    "suspend-react": "0.1.3",
    "swiper": "11.0.7",
    "tailwindcss": "^3.4.1",
    "tailwindcss-selection-variant": "^0.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/types": "^19.0.3",
    "@types/node": "20.11.28",
    "@types/react": "18.2.66",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "autoprefixer": "10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-mocha": "10.4.1",
    "eslint-plugin-more": "^1.0.5",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "8.4.36",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "2.4.12",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "typescript": "5.4.2"
  }
}
